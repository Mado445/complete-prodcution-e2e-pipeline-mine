pipeline{
    agent{
        label "jenkins-agent"
    }

    tools{
        JDK "Java17"
        maven "Maven3"
    }

    stages{
        stage("Cleanup Workspace"){
            steps{
                CleanWs()
            }
        }
    }

    stages{
        stage("Checkout from SCM"){
            steps{
                git branch: 'main', credentialsId: 'github-ref', url: 'https://github.com/dmancloud/complete-prodcution-e2e-pipeline'
            }

        }
    }





}